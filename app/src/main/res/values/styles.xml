<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CustomTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/colorCustomPrimary</item>
        <item name="colorPrimaryDark">@color/colorCustomDarkPrimary</item>
        <item name="colorAccent">@color/colorCustomAccent</item>
    </style>

    <style name="InfoStyle">
        <item name="background">@drawable/rounded_text_view_green</item>
    </style>



</resources>